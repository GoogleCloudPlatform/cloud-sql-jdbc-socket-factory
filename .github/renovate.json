{
  "extends": [
    "config:base",
    ":semanticCommitTypeAll(chore)"
  ],
  "packageRules": [
    {
      "packageNames":[ "mysql:mysql-connector-java" ],
      "enabled": false
    },
    {
      "packageNames": ["com.google.guava:guava"],
      "allowedVersions": "/.+-android/"
    },
    {
      "packageNames": ["com.microsoft.sqlserver:mssql-jdbc"],
      "allowedVersions": "/.+jre8.?/"
    }
  ],
  "prConcurrentLimit": 0,
  "rebaseStalePrs": true,
  "masterIssue": true
}
